//Full diamond zombie with diamond axe and shield
/setblock ~ ~ ~ mob_spawner 0 replace {SpawnData:{id:zombie,HandItems:[{Count:1,id:diamond_axe},{Count:1,id:shield}],ArmorItems:[{Count:1,id:diamond_boots},{Count:1,id:diamond_leggings},{Count:1,id:diamond_chestplate},{Count:1,id:diamond_helmet}]},SpawnRange:2,Delay:200,MaxSpawnDelay:600}
//Full iron skeleton with iron sword
/setblock ~ ~ ~ mob_spawner 0 replace {SpawnData:{id:skeleton,HandItems:[{Count:1,id:iron_pickaxe},{}],ArmorItems:[{Count:1,id:iron_boots},{Count:1,id:iron_leggings},{Count:1,id:iron_chestplate},{Count:1,id:iron_helmet}]},SpawnRange:1,Delay:0,MaxSpawnDelay:300}
//setblock to a chest with any loot table applied
/setblock ~ ~ ~ minecraft:chest 2 replace {LootTable:"chests/simple_dungeon"}